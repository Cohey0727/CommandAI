{
  "name": "command-ai",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "dev": "concurrently \"pnpm run solid\" \"pnpm run watch:css\"",
    "solid": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "codegen:css": "tcm src --pattern \"**/*.module.css\" -d src -o src/styles.d.ts",
    "watch:css": "tcm src --pattern \"**/*.module.css\" -w",
    "check": "tsc --noEmit"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.16",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.3",
    "solid-devtools": "^0.29.3",
    "ts-node": "^10.9.2",
    "typed-css-modules": "^0.9.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-solid": "^2.9.1"
  },
  "dependencies": {
    "@solid-primitives/destructure": "^0.1.15",
    "@solidjs/router": "^0.11.3",
    "clsx": "^2.1.0",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.8.12"
  }
}
